{
    "version": 2,
    "builds": [
        {
            "src": "api.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "api.py",
            "headers": {
                "Access-Control-Allow-Origin": "*"
        }}
    ]
}